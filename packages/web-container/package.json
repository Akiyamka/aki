  "version": "0.0.1",
  "private": false,
  "type": "module",
  "source": "./src/index.ts",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.mjs",
  "unpkg": "lib/index.umd.min.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/index.esm.mjs",
      "default": "./lib/index.esm.mjs"
    },
    "./plugins/*": {
      "types": "./lib/plugins/*.d.ts",
      "require": "./lib/plugins*.js",
      "import": "./lib/plugins/*.mjs",
      "default": "./lib/plugins/*.mjs"
    }
  },
  "scripts": {
    "test": "tsc",
    "build": "microbundle ./src/**/*.ts"
  },
  "dependencies": {
    "nanoid": "^4.0.2",
    "p-defer": "^4.0.0",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
